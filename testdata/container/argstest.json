{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "expansion-pod"
  },
  "spec": {
    "containers": [
      {
        "name": "test-container",
        "image": "quay.io/openshifttest/hello-openshift@sha256:424e57db1f2e8e8ac9087d2f5e8faea6d73811f0b6f96301bc94293680897073",
        "args": [
          "/bin/sh",
          "-c",
          "echo $(PUBLIC_URL)"
        ],
        "env": [
          {
            "name": "PUBLIC_URL",
            "value": "http"
          }
        ]
      }
    ],
    "restartPolicy": "Never"
  }
}
