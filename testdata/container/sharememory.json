{
    "apiVersion": "v1",
    "id": "hello-openshift",
    "kind": "Pod",
    "metadata": {
        "name": "hello-openshift",
        "labels": {
            "name": "hello-openshift"
        }
    },
    "spec": {
        "volumes": [
            {
                "name": "dshm",
                "emptyDir": {
                    "medium": "Memory"
                }
            }
        ],
        "containers": [
            {
                "image": "quay.io/openshifttest/pause@sha256:b31bfb4d0213f254d361e0079deaaebefa4f82ba7aa76ef82e90b4935ad5b105",
                "name": "hello-container1",
                "ports": [
                    {
                        "containerPort": 8080
                    }
                ],
                "volumeMounts": [
                    {
                        "mountPath": "/dev/shm",
                        "name": "dshm"
                    }
                ]
            },
            {
                "image": "quay.io/openshifttest/pause@sha256:b31bfb4d0213f254d361e0079deaaebefa4f82ba7aa76ef82e90b4935ad5b105",
                "name": "hello-container2",
                "ports": [
                    {
                        "containerPort": 8081
                    }
                ],
                "volumeMounts": [
                    {
                        "mountPath": "/dev/shm",
                        "name": "dshm"
                    }
                ]
            }
        ]
    }
}
