{
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
        "name": "#POD_NAME#",
        "labels": {
            "name": "frontendhttp"
        }
    },
    "spec": {
        "containers": [{
            "name": "myfrontend",
            "image": "quay.io/openshifttest/storage@sha256:a05b96d373be86f46e76817487027a7f5b8b5f87c0ac18a246b018df11529b40",
            "imagePullPolicy": "IfNotPresent",
            "ports": [{
                "containerPort": 80,
                "name": "http-server"
            }],
            "volumeMounts": [{
                "mountPath": "/mnt/nfs",
                "name": "pvol"
            }]
        }],
        "securityContext": {
            "runAsUser": "#USER_ID#",
            "supplementalGroups": ["#GROUP#"]
        },
        "volumes": [{
            "name": "pvol",
            "nfs": {
                "server": "10.66.79.133",
                "path": "/home/data/510690",
                "readOnly": false
            }
        }]
    }
}
