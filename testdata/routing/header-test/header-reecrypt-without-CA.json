{
  "apiVersion": "v1",
  "items": [
    {
      "kind": "Route",
      "apiVersion": "route.openshift.io/v1",
      "metadata": {
        "name": "header-reencrypt"
      },
      "spec": {
        "to": {
          "kind": "Service",
          "name": "service-reen"
        },
        "tls": {
          "termination": "reencrypt"
        }
      }
    },
    {
      "kind": "Service",
      "apiVersion": "v1",
      "metadata": {
        "name": "service-reen",
        "annotations": {
          "service.alpha.openshift.io/serving-cert-secret-name": "header-cert"
        }
      },
      "spec": {
        "ports": [
          {
            "name": "https",
            "protocol": "TCP",
            "port": 27443,
            "targetPort": 8443
          }
        ],
        "selector": {
          "name": "header-test-reen"
        }
      }
    },
    {
      "kind": "Pod",
      "apiVersion": "v1",
      "metadata": {
        "name": "header-test-reen",
        "labels": {
          "name": "header-test-reen"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "header-test-pod",
            "image": "quay.io/openshifttest/header-test-for-reencrypt@sha256:fce41aacb75a0f8a39e0b29d8e765ca2064a4cf27a93680eeebd564321a9c7de",
            "ports": [
              {
                "containerPort": 8080
              },
              {
                "containerPort": 8443
              }
            ],
            "volumeMounts": [
              {
                "name": "cert",
                "mountPath": "/srv/header-test/config/"
              }
            ]
          }
        ],
        "volumes": [
          {
            "name": "cert",
            "secret": {
              "secretName": "header-cert"
            }
          }
        ]
      }
    }
  ],
  "kind": "List"
}
