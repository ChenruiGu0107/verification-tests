---
login:
  url: /login
  elements:
    - selector:
        id: j_username
      op: send_keys <username>
      type: input
    - selector:
        name: j_password
      op: send_keys <password>
      type: input
  elements:
    - selector:
        id: yui-gen1-button
      op: click
      type: button
  action: verify_logged_in

verify_logged_in:
  elements:
  - selector:
      text: Jenkins Admin
    visible: true
