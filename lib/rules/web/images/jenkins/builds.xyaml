build_with_string_parameter:
  url: /job/<job_name>
  elements:
  - selector:
      text: Build with Parameters
    type: a
    op: click
  - selector:
      name: value
      class: setting-input
    type: input
    op: send_keys <build_parameter>
  - selector:
      text: Build
    type: button
    op: click

build_now:
  url: /job/<job_name>
  elements:
  - selector:
    text: Build Now
  op: click
  type: a
